{
  "title":"SmartNotebook",
  "plugins": [
	"livereload",
    "splitter",
    "hide-element",
    "code",
    "theme-door",
    "-lunr",
    "-search",
    "-sharing",
    "search-pro-fixed", 
    "image-captions",
    "flexible-alerts",
    "mathjax-pro",
    "-back-to-top-button"
  ],
  "variables": {
    "doorTheme": {
      "nav": [
        {
          "url": "http://smartnotebook.tech/",
          "target": "_blank",
          "name": "产品首页"
        }
      ]
    }
  },
  "pluginsConfig": {
    "hide-element": {
      "elements": [".gitbook-link"]
    },
    "flexible-alerts": {
      "note": {
        "label": "Hinweis"
      },
      "tip": {
        "label": "Tipp"
      },
      "warning": {
        "label": "Warnung"
      },
      "danger": {
        "label": "Achtung"
      }
    },
    "doorTheme": {
      "search-placeholder": "请输入关键字搜索",
      "logo": "./SmartNotebook.svg",
      "favicon": "./SmartNotebook.ico"
    }
  },
  "styles": {
    "website": "styles/website.css",
    "ebook": "styles/ebook.css",
    "pdf": "styles/pdf.css",
    "mobi": "styles/mobi.css",
    "epub": "styles/epub.css"
  }
}





